
<table>
  <thead>
    <tr>
      <th>Properties</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_profiles.each do |user_profile| %>
      <tr>
        <td><%= user_profile.properties %></td>
        <td><%= user_profile.user_id %></td>
        <td><%= link_to 'Show', user_profile %></td>
        <td><%= link_to 'Add friend', friendship_path(id: user_profile.user_id) %></td>
        <td><%= link_to 'Destroy', user_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

