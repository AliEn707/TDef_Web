

<%= content_tag :table, class:"table table-striped" do -%>
	<%= content_tag :tr do -%>
		<%map_columns.each do |m_m|%>
			<%= content_tag :th do -%>
				<%=m_m%>
			<%end%>
		<%end%>
	<%end%>
	<%@maps.each do |m_m|%>
		<%= content_tag :tr do -%>
			<%= content_tag :td do -%>
				<%=image_tag(""+m_m.icon, height: '128', width: '256')%>
			<%end%>
			<%= content_tag :td do -%>
				<%= m_m.name%>
			<%end%>
			<%= content_tag :td do -%>
				<= m_m.completed%>
			<%end%>
			<%= content_tag :td do -%>
				<%=link_to "Edit", "/map/edit/?id="+m_m.id.to_s%>
			<%end%>
		<%end%>
	<%end%>
<%end%>