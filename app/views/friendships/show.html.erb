<h2>Friends outcome</h2>
<ul>
  <% for friendship in @user.friendships %>
    <li>
      <%=h friendship.friend.name %>
      (<%= link_to "remove", friendship, :method => :delete %>)
    </li>
  <% end %>
</ul>

<p><= link_to "Find Friends", users_path %></p>

<h2>Friends income</h2>
<ul>
  <% for user in @user.friends_income %>
    <li><%=h user.name %></li>
  <% end %>
</ul>

<h2>Friends</h2>
<ul>
  <% for user in @user.friends %>
    <li><%=h user.name %></li>
  <% end %>
</ul>

